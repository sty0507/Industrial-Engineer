@charset "UTF-8";
header{
	position: fixed;
	top: 0px;
	height: 40px;
	width: 100%;
	background-color: blue;
	text-align: center;
	color: white;
	font-size: 25px;
}
nav{
	position: fixed;
	top: 40px;
	height: 30px;
	width: 100%;
	background-color: lightblue;
	line-height: 30px;
}
footer{
	position:fixed;
	bottom: 0px;
	height: 30px;
	width: 100%;
	background-color: blue;
	text-align: center;
	color: white;
}
section{
	position: fixed;
	top: 70px;
	width: 100%;
	height: 100%;
	background-color: lightgray;
}
section > p{
	padding-left: 10px;
}
section > h1{
	text-align: center;
}
section  > table{
	text-align:center;
}
section >  table {
    margin-left:auto; 
    margin-right:auto;
}
section  > table, td, th {
    border-collapse : collapse;
    border : 1px solid black;
};